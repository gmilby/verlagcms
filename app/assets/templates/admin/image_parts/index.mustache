<div id="overlay">
  <div class="modal image-browser">
    <h2>Image Browser</h2>
    <ul class="assets">
    {{#assets}}
      <li id="asset-{{id}}">
        <div class="image">
          <a href="{{admin_path}}/edit{{query_path}}" id="edit-asset-{{id}}">
            <img src="/images/{{id}}/{{file_name}}?w=120&amp;h=90&amp;c=t&amp;g=North" alt="{{title}}" />
          </a>
        </div>
        <p>{{title}}<br />
      </li>
    {{/assets}}
    </ul>
  </div>
</div>
