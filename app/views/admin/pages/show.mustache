{{#page}}  
<div id="preview-{{id}}" class="preview">
  <iframe src="/preview{{path}}?edit=true" id="page-iframe-{{id}}"></iframe>
</div>   

<div class="toolbar">   
  <h1 id="page-title-{{id}}">
    {{title}}
    <a class="toggle-page-editor edit" href="{{admin_path}}/edit">Edit</a>
  </h1>
</div>
{{/page}}
