{{#page}}  
<div id="preview-{{id}}" class="preview">
  <iframe src="/preview{{path}}?edit=true" id="page-iframe-{{id}}"></iframe>
</div>   

<div class="field page-title">   
  <h1 id="page-title-{{id}}">{{title}}</h1>
  <a class="toggle-page-editor" href="{{admin_path}}/edit">Edit Page</a>
</div>
{{/page}}
