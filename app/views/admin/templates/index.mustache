<div id="layouts">
{{# templates }}
  <h2>{{ title }} <span class="modify"><a href="/admin/design/new/layout">+</a></span></h2> 
  <ul>
    {{# models }}
      <li class="layout" id="layout-{{id}}"> 
        <div class="item"> 
          <span class="title"> 
             <a id id="edit-layout-{{id}}" href="{{admin_path}}">{{name}}</a>       
          </span> 
          <span class="modify">                 
            <a class="remove-layout" id="remove-layout-{{id}}" href="{{admin_path}}/{{id}}/remove">x</a> 
          </span>  
        </div>
      </li>  
    {{/ models }}
  </ul>   
{{/ templates }}  
</div>
